{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "outDir": "./build",
    "typeRoots": ["./node_modules/@types", "./node_modules/@altv"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "removeComments": true,
  }
}